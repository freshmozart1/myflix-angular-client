{
    "recurseDepth": 10,
    "source": {
        "include": [
            "src"
        ],
        "includePattern": ".+\\.ts$",
        "excludePattern": "(^|\\/|\\\\)_|\\.spec\\.ts$"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "plugins": [
        "node_modules/better-docs/typescript"
    ],
    "opts": {
        "destination": "./docs/jsdoc",
        "recurse": true
    }
}